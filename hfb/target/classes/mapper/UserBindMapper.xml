<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.heepay.mapper.UserBindMapper">


	<update id="updateStatus">
			update
				user_bind
			set
				status = 1
			where
				agent_user_id = #{id}
	</update>
</mapper>

